<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/frag20"
    android:background="@color/white"
    tools:context=".Fragment_1">

    <include layout="@layout/back_lay"></include>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Benefits"
            android:textStyle="bold"
            android:padding="5dp"
            android:textSize="32sp"
            android:layout_marginTop="15dp"
            android:textColor="@color/white"
            android:background="@drawable/frag14_img"
            android:id="@+id/frag20_headtxt2"
            android:layout_below="@+id/frag20_headtxt"
            android:layout_alignLeft="@+id/frag20_tableimg"
            android:layout_alignStart="@+id/frag20_tableimg" />

        <ImageView
            android:layout_width="950dp"
            android:layout_height="450dp"
            android:id="@+id/frag20_tableimg"
            android:src="@drawable/frag20_img"
            android:layout_marginLeft="90dp"
            android:layout_marginStart="90dp"
            android:layout_marginTop="20dp"
            android:layout_below="@+id/frag20_headtxt2"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="* Data in File"
            android:id="@+id/textView6"
            android:textColor="@color/black"
            android:layout_below="@+id/frag20_femaleimg"
            android:layout_toRightOf="@+id/frag20_headtxt2"
            android:layout_toEndOf="@+id/frag20_headtxt2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Energize mitochondria and vitalize fertility"
            android:id="@+id/frag20_headtxt"
            android:layout_marginTop="20dp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="35sp"
            android:layout_alignParentTop="true"
            android:layout_alignLeft="@+id/frag20_headtxt2"
            android:layout_alignStart="@+id/frag20_headtxt2" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView4"
            android:layout_below="@+id/frag20_tableimg"
            android:layout_toLeftOf="@+id/textView6"
            android:layout_toStartOf="@+id/textView6"
            android:layout_marginRight="93dp"
            android:layout_marginEnd="93dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/frag20_maleimg"
            android:layout_marginTop="20dp"
            android:src="@drawable/frag20_maleimg"
            android:layout_below="@+id/imageView4"
            android:layout_toLeftOf="@+id/textView6"
            android:layout_toStartOf="@+id/textView6" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/frag20_femaleimg"
            android:src="@drawable/frag20_femaleimg"
            android:layout_alignBottom="@+id/frag20_maleimg"
            android:layout_toRightOf="@+id/textView6"
            android:layout_toEndOf="@+id/textView6" />


    </RelativeLayout>


</FrameLayout>
