<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/frag9"
    android:background="@color/white"
    tools:context=".Fragment_1">

    <include layout="@layout/back_lay"></include>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:layout_width="700dp"
            android:layout_height="100dp"
            android:layout_marginLeft="40dp"
            android:id="@+id/frag10_headimg"
            android:scaleType="fitXY"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            android:src="@drawable/frag9_img1"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="570dp"
            android:layout_height="280dp"
            android:id="@+id/imageView18"
            android:scaleType="fitXY"
            android:src="@drawable/frag9_img2"
            android:layout_below="@+id/frag10_headimg"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="198dp"
            android:layout_marginEnd="198dp" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="43dp"
            android:id="@+id/imageView19"
            android:paddingTop="2dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:src="@drawable/bullet_img"
            android:layout_below="@+id/frag10_headimg"
            android:layout_alignLeft="@+id/frag10_headimg"
            android:layout_alignStart="@+id/frag10_headimg" />

        <TextView
            android:layout_width="400dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Corrects the energy starvation of myoctyes"
            android:id="@+id/frag10_pt1_txt"
            android:textColor="@color/black"
            android:textSize="35sp"
            android:layout_marginLeft="5dp"
            android:layout_alignTop="@+id/imageView19"
            android:layout_toRightOf="@+id/imageView19"
            android:layout_toEndOf="@+id/imageView19" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="43dp"
            android:id="@+id/imageView20"
            android:paddingTop="2dp"
            android:layout_marginTop="13dp"
            android:layout_below="@+id/frag10_pt1_txt"
            android:layout_alignLeft="@+id/imageView19"
            android:layout_alignStart="@+id/imageView19"
            android:src="@drawable/bullet_img" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Helps maintains optimal mitochondrial and cellular functions of cardiac muscles"
            android:id="@+id/frag10_pt2_txt"
            android:textColor="@color/black"
            android:textSize="35sp"
            android:layout_alignTop="@+id/imageView20"
            android:layout_alignLeft="@+id/frag10_pt1_txt"
            android:layout_alignStart="@+id/frag10_pt1_txt"
            android:layout_alignRight="@+id/frag10_pt1_txt"
            android:layout_alignEnd="@+id/frag10_pt1_txt" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="43dp"
            android:id="@+id/imageButton"
            android:paddingTop="2dp"
            android:layout_marginTop="13dp"
            android:layout_below="@+id/frag10_pt2_txt"
            android:layout_alignLeft="@+id/imageView20"
            android:layout_alignStart="@+id/imageView20"
            android:src="@drawable/bullet_img" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Helps improves ejection fraction, exercise tolerance, cardiac output and stroke volume"
            android:id="@+id/frag10_pt3_txt"
            android:textColor="@color/black"
            android:textSize="35sp"
            android:layout_alignTop="@+id/imageButton"
            android:layout_alignLeft="@+id/frag10_pt2_txt"
            android:layout_alignStart="@+id/frag10_pt2_txt"
            android:layout_alignRight="@+id/imageView18"
            android:layout_alignEnd="@+id/imageView18" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="43dp"
            android:id="@+id/imageView21"
            android:paddingTop="2dp"
            android:layout_marginTop="3dp"
            android:layout_below="@+id/frag10_pt3_txt"
            android:layout_alignLeft="@+id/imageButton"
            android:layout_alignStart="@+id/imageButton"
            android:src="@drawable/bullet_img" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Helps reduces MACE and cardiovascular death by 43%"
            android:id="@+id/textView10"
            android:textSize="35sp"
            android:textColor="@color/black"
            android:layout_alignTop="@+id/imageView21"
            android:layout_alignLeft="@+id/frag10_pt3_txt"
            android:layout_alignStart="@+id/frag10_pt3_txt" />


    </RelativeLayout>


</FrameLayout>
