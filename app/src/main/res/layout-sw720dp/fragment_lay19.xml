<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/frag6"
    android:background="@color/white"
    tools:context=".Fragment_1">

    <include layout="@layout/back_lay"></include>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="UltraSOL"
            android:textSize="30sp"
            android:textStyle="bold"
            android:textColor="#000080"
            android:id="@+id/frag20_headtxt2"
            android:layout_alignTop="@+id/textView5"
            android:layout_toLeftOf="@+id/textView5"
            android:layout_toStartOf="@+id/textView5" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="\timproves Bio-Availability"
            android:id="@+id/textView5"
            android:textColor="#000080"
            android:textSize="30sp"
            android:layout_marginTop="10dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

        <ImageView
            android:layout_width="700dp"
            android:layout_height="350dp"
            android:id="@+id/frag20_tableimg"
            android:layout_marginBottom="-15dp"
            android:src="@drawable/frag6_img1"
            android:layout_below="@+id/frag20_headtxt2"
            android:layout_alignRight="@+id/imageView10"
            android:layout_alignEnd="@+id/imageView10"
            android:layout_marginRight="96dp"
            android:layout_marginEnd="96dp" />

        <ImageView
            android:layout_width="390dp"
            android:layout_height="270dp"
            android:id="@+id/imageView9"
            android:src="@drawable/frag6_img2"
            android:layout_below="@+id/frag20_tableimg"
            android:layout_toLeftOf="@+id/textView6"
            android:layout_toStartOf="@+id/textView6"
            android:layout_marginTop="30dp" />

        <ImageView
            android:layout_width="390dp"
            android:layout_height="270dp"
            android:id="@+id/imageView10"
            android:src="@drawable/frag6_img3"
            android:layout_alignTop="@+id/imageView9"
            android:layout_toRightOf="@+id/textView6"
            android:layout_toEndOf="@+id/textView6" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="* Data in File"
            android:id="@+id/textView6"
            android:layout_marginBottom="10dp"
            android:textColor="@color/black"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true" />


    </RelativeLayout>


</FrameLayout>
