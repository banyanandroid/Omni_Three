<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/frag12"
    android:background="@color/white"
    tools:context=".Fragment_1">

    <include layout="@layout/back_lay"></include>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center">


        <ImageView
            android:layout_width="800dp"
            android:layout_height="120dp"
            android:scaleType="fitXY"
            android:layout_marginTop="10dp"
            android:id="@+id/frag10_headimg"
            android:src="@drawable/frag12_img"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="34dp"
            android:layout_marginStart="34dp" />



        <ImageView
            android:layout_width="20dp"
            android:layout_height="30dp"
            android:id="@+id/imageView19"
            android:paddingTop="2dp"
            android:padding="1dp"
            android:layout_marginTop="70dp"
            android:layout_marginLeft="80dp"
            android:src="@drawable/bullet_img"
            android:layout_below="@+id/frag10_headimg" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Meta-analysis confirms- CoQ 10 can reduce systolic BP up to 17 mg and diastolic by 10 mg in Hypertensive patients."
            android:id="@+id/frag10_pt1_txt"
            android:textColor="@color/black"
            android:layout_marginLeft="10dp"
            android:textSize="32sp"
            android:layout_alignTop="@+id/imageView19"
            android:layout_toRightOf="@+id/imageView19"
            android:layout_alignRight="@+id/frag10_headimg"
            android:layout_alignEnd="@+id/frag10_headimg" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="30dp"
            android:id="@+id/imageView20"
            android:padding="1dp"
            android:paddingTop="2dp"
            android:layout_marginTop="17dp"
            android:layout_below="@+id/frag10_pt1_txt"
            android:layout_alignLeft="@+id/imageView19"
            android:layout_alignStart="@+id/imageView19"
            android:src="@drawable/bullet_img" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Helps regulate glycemic control in Type 2 diabetes"
            android:id="@+id/frag10_pt2_txt"
            android:textColor="@color/black"
            android:textSize="32sp"
            android:layout_alignTop="@+id/imageView20"
            android:layout_alignLeft="@+id/frag10_pt1_txt"
            android:layout_alignStart="@+id/frag10_pt1_txt"
            android:layout_alignRight="@+id/frag10_pt1_txt"
            android:layout_alignEnd="@+id/frag10_pt1_txt" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="30dp"
            android:id="@+id/imageButton"
            android:padding="1dp"
            android:paddingTop="2dp"
            android:layout_marginTop="17dp"
            android:layout_below="@+id/frag10_pt2_txt"
            android:layout_alignLeft="@+id/imageView20"
            android:layout_alignStart="@+id/imageView20"
            android:src="@drawable/bullet_img" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Decreases lipoprotein (a)"
            android:id="@+id/frag10_pt3_txt"
            android:textColor="@color/black"
            android:textSize="32sp"
            android:layout_alignTop="@+id/imageButton"
            android:layout_alignLeft="@+id/frag10_pt2_txt"
            android:layout_alignStart="@+id/frag10_pt2_txt"
            android:layout_alignRight="@+id/frag10_pt1_txt"
            android:layout_alignEnd="@+id/frag10_pt1_txt" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="30dp"
            android:id="@+id/imageView21"
            android:padding="1dp"
            android:paddingTop="2dp"
            android:layout_marginTop="17dp"
            android:layout_below="@+id/frag10_pt3_txt"
            android:layout_alignLeft="@+id/imageButton"
            android:layout_alignStart="@+id/imageButton"
            android:src="@drawable/bullet_img" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Decreases concentration of monocyte adhesion molecules and reduces plaque instability"
            android:id="@+id/frag10_pt4_txt"
            android:textColor="@color/black"
            android:textSize="32sp"
            android:layout_alignTop="@+id/imageView21"
            android:layout_alignLeft="@+id/frag10_pt3_txt"
            android:layout_alignStart="@+id/frag10_pt3_txt"
            android:layout_alignRight="@+id/frag10_pt1_txt"
            android:layout_alignEnd="@+id/frag10_pt1_txt" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Additional benefits"
            android:id="@+id/textView7"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="35sp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="20dp"
            android:layout_below="@+id/frag10_headimg"
            android:layout_alignLeft="@+id/frag10_headimg"
            android:layout_alignStart="@+id/frag10_headimg" />


    </RelativeLayout>


</FrameLayout>
