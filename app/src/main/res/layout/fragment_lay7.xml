<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/frag7"
    android:background="@color/white"
    tools:context=".Fragment_1">

    <include layout="@layout/back_lay"></include>

<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:layout_width="300dp"
        android:layout_height="wrap_content"

        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Energize mitochondria, vitalize cardiac cells"
        android:id="@+id/frag7_headtxt1"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginLeft="39dp"
        android:layout_marginTop="52dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="39dp" />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="25dp"
        android:layout_marginTop="20dp"
        android:padding="1dp"
        android:id="@+id/imageView11"
        android:layout_below="@+id/frag7_headtxt1"
        android:layout_alignLeft="@+id/frag7_headtxt1"
        android:layout_alignStart="@+id/frag7_headtxt1"
        android:src="@drawable/bullet_img" />

    <TextView
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Essential for cellular energy generation"
        android:id="@+id/textView9"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@+id/imageView11"
        android:layout_toEndOf="@+id/imageView11"
        android:layout_alignTop="@+id/imageView11" />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="25dp"
        android:padding="1dp"
        android:id="@+id/imageView12"
        android:src="@drawable/bullet_img"
        android:contentDescription="image"
        android:layout_below="@+id/textView9"
        android:layout_toLeftOf="@+id/textView9"
        android:layout_toStartOf="@+id/textView9" />

    <ImageView
        android:layout_width="290dp"
        android:layout_height="230dp"
        android:id="@+id/imageView13"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="5dp"
        android:src="@drawable/frag7_img"
        android:layout_above="@+id/textView36"
        android:layout_toRightOf="@+id/textView35"
        android:layout_toEndOf="@+id/textView35" />

    <TextView
        android:layout_width="700dp"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Level peaks at 20 years and at 45 the reserves would have depleted by 50%"
        android:id="@+id/textView36"
        android:textStyle="bold"
        android:layout_marginTop="10dp"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/imageView12"
        android:layout_alignStart="@+id/imageView12" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:layout_marginTop="10dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="High Risk individuals"
        android:id="@+id/textView37"
        android:textStyle="bold"
        android:layout_marginLeft="100dp"
        android:layout_below="@+id/textView36"
        android:layout_alignLeft="@+id/textView36"
        android:layout_alignStart="@+id/textView36" />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="25dp"
        android:padding="1dp"
        android:layout_marginTop="10dp"
        android:id="@+id/imageView14"
        android:layout_below="@+id/textView37"
        android:layout_alignLeft="@+id/textView37"
        android:layout_alignStart="@+id/textView37"
        android:src="@drawable/bullet_img" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Insufficient dietary intake"
        android:id="@+id/textView38"
        android:layout_marginLeft="5dp"
        android:layout_alignBottom="@+id/imageView14"
        android:layout_toRightOf="@+id/imageView14"
        android:layout_toEndOf="@+id/imageView14" />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="25dp"
        android:padding="1dp"
        android:layout_marginTop="5dp"
        android:id="@+id/imageView15"
        android:layout_below="@+id/imageView14"
        android:layout_alignLeft="@+id/imageView14"
        android:layout_alignStart="@+id/imageView14"
        android:src="@drawable/bullet_img" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Impaired CoQ 10 bio-synthesis due to B6 deficiency"
        android:id="@+id/textView39"
        android:layout_alignTop="@+id/imageView15"
        android:layout_alignLeft="@+id/textView38"
        android:layout_alignStart="@+id/textView38" />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="25dp"
        android:padding="1dp"
        android:layout_marginTop="5dp"
        android:id="@+id/imageView16"
        android:layout_below="@+id/textView39"
        android:layout_alignLeft="@+id/imageView15"
        android:layout_alignStart="@+id/imageView15"
        android:src="@drawable/bullet_img" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Poor GI absorption"
        android:id="@+id/textView40"
        android:layout_alignTop="@+id/imageView16"
        android:layout_alignLeft="@+id/textView39"
        android:layout_alignStart="@+id/textView39" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Powerful immune enhancer"
        android:id="@+id/textView35"
        android:layout_alignTop="@+id/imageView12"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9" />

</RelativeLayout>

</FrameLayout>
